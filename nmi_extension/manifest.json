{
  "name": "NMI Wesearch",
  "description": "Allows users to submit links to NMI Research tool",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs", 
    "http://*/*", 
    "https://*/*"
  ],

  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },

  "browser_action": {
      "default_title": "Submit Page to NMI wesearch",
      "default_icon": "icon.png"
  },

  "manifest_version": 2
}
